<?xml version='1.0' encoding='utf-8'?>
<widget id="com.robbertvanbeek.meistro" version="3.0.1" versionCode="75" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Meistro</name>
    <description>Meistro app van De Harmonie Barneveld, vol informatie over Meistro.</description>
    <author email="info@deharmoniebarneveld.nl" href="www.deharmoniebarneveld.nl">De Harmonie Barneveld</author>
    <content src="index.html" />
    <icon src="icon.png" />
    <icon gap:platform="android" gap:qualifier="ldpi" src="icon-ldpi.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="icon-mdpi.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="icon-hdpi.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="icon-xhdpi.png" />
    <icon gap:platform="android" gap:qualifier="xxhdpi" src="icon-xxhdpi.png" />
    <icon gap:platform="android" gap:qualifier="xxxhdpi" src="icon-xxxhdpi.png" />
    <icon gap:platform="ios" height="57" src="icon.png" width="57" />
    <icon gap:platform="ios" height="114" src="Icon-2x.png" width="114" />
    <icon gap:platform="ios" height="60" src="Icon-60.png" width="60" />
    <icon gap:platform="ios" height="72" src="Icon-72.png" width="72" />
    <icon gap:platform="ios" height="144" src="Icon-72-2x.png" width="144" />
    <icon gap:platform="ios" height="76" src="Icon-76.png" width="76" />
    <icon gap:platform="ios" height="152" src="Icon-76-2x.png" width="152" />
    <icon gap:platform="ios" height="50" src="Icon-Small-50.png" width="50" />
    <icon gap:platform="ios" height="100" src="Icon-Small-50-2x.png" width="100" />
    <icon gap:platform="ios" height="120" src="Icon-60-2x.png" width="120" />
    <gap:splash gap:platform="ios" height="480" src="Default.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="Default2x.png" width="640" />
    <gap:splash gap:platform="ios" height="1136" src="Default-568h2x.png" width="640" />
    <gap:splash gap:platform="ios" height="1134" src="Default-667h2x.png" width="750" />
    <gap:splash gap:platform="ios" height="1024" src="Default-Portrait" width="768" />
    <gap:splash gap:platform="ios" height="2048" src="Default-Portrait2x" width="1536" />
    <gap:splash gap:platform="ios" height="2208" src="Default-Portrait-736h3x" width="1242" />
    <gap:plugin name="org.apache.cordova.network-information" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <access origin="*" />
    <gap:config-file overwrite="true" parent="UIStatusBarHidden" platform="ios">
        <true />
    </gap:config-file>
    <gap:config-file overwrite="true" parent="UIViewControllerBasedStatusBarAppearance" platform="ios">
        <false />
    </gap:config-file>
    <preference name="permissions" value="none" />
    <preference name="orientation" value="portrait" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="7" />
    <preference name="android-installLocation" value="auto" />
</widget>
